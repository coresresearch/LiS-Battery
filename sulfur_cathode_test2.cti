#-------------------------------------------------------------------------------
#  Reaction data  --------------------------------------------------------------
#-------------------------------------------------------------------------------


#-------------------------------------------------------------------------------
# Anode reactions
#-------------------------------------------------------------------------------
surface_reaction("Li(s) <=> Li+(e) + electron", [6e5, 0.0, 0.0], id="Anode-1", beta=0.5)

#-------------------------------------------------------------------------------
# Sulfur-electrolyte reactions
#-------------------------------------------------------------------------------
surface_reaction("S8(s) <=> S8(e)", [1.900e-5, 0.0, 0.0], id="S-E-1")

#-------------------------------------------------------------------------------
# Carbon Electrolyte reactions
#-------------------------------------------------------------------------------

surface_reaction("0.5 S8(e) + electron <=> 0.5 S8-(e)", [1e4, 0.0, 0.0], id="C-E-1", beta=0.5)
# 7.725

surface_reaction("1.5 S8-(e) + electron <=> 2 S6-(e)", [5.331e4, 0.0, 0.0], id="C-E-2", beta=0.5)

surface_reaction("S6-(e) + electron <=> 1.5 S4-(e)", [1.193e1, 0.0, 0.0], id="C-E-3", beta=0.5)

surface_reaction("0.5 S4-(e) + electron <=> S2-(e)", [6.375e-2, 0.0, 0.0], id="C-E-4", beta=0.5)

surface_reaction("0.5 S2-(e) + electron <=> S-(e)", [5.655e2, 0.0, 0.0], id="C-E-5", beta=0.5)

